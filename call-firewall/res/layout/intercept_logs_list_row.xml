<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
    
    <LinearLayout
	    android:orientation="vertical"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content">
	    
		<TextView android:id="@+id/txt_intercept_name" 
				xmlns:android="http://schemas.android.com/apk/res/android" 
				android:layout_width="wrap_content" 
				android:layout_height="wrap_content" 
				android:textSize="30dip" 
				android:text="@string/app_empty_list"/>
				
		<TextView android:id="@+id/txt_intercept_phone" 
				xmlns:android="http://schemas.android.com/apk/res/android" 
				android:layout_width="wrap_content" 
				android:layout_height="wrap_content" 
				android:textSize="20dip"/>
    </LinearLayout>
    
	    
		<TextView android:id="@+id/txt_intercept_content" 
				xmlns:android="http://schemas.android.com/apk/res/android" 
				android:layout_width="wrap_content" 
				android:layout_height="wrap_content" 
				android:textSize="30dip" 
				android:text="@string/app_empty_list"/>
				
		<TextView android:id="@+id/txt_intercept_create_at" 
				xmlns:android="http://schemas.android.com/apk/res/android" 
				android:layout_width="wrap_content" 
				android:layout_height="wrap_content" 
				android:textSize="20dip"/>
</LinearLayout>